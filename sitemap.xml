<!-- https://www.sitemaps.org/protocol.html -->
<!-- Each sitemap file can be no more than 50,000 URLs or larger than 50MB (52,428,800 bytes) -->
<!-- Create multiple sitemaps using a sitemap index -->
<!-- Must be at the root of the domain -->

<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- Add every page (can be auto generated using a tool) -->
   <url>
      <!-- You must escape entities & non-ASCII characters in URLs -->
      <loc>http://www.example.com/</loc>
      <!-- Below tags are optional -->
      <lastmod>2005-01-01</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.8</priority>
   </url>

</urlset> 